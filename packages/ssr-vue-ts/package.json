{
  "name": "@velocejs/ssr-vue-ts",
  "private": true,
  "version": "0.0.1",
  "description": "Generated by velocejs",
  "scripts": {
    "dev": "pnpm ts -- ./dev-server.ts",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:ts": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr app/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && pnpm run build:server && node prerender",
    "serve": "cross-env NODE_ENV=production node prod-server",
    "debug": "node --inspect-brk server",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "ts": "node -r esbuild-register"
  },
  "dependencies": {
    "@velocejs/server": "workspace:^0.6.11",
    "mime-types": "^2.1.35",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/server-renderer": "^3.2.31",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dep-import-type": "link:./dep-import-type",
    "esbuild-register": "^3.3.2",
    "express": "^4.17.3",
    "happy-dom": "^2.55.0",
    "serve-static": "^1.15.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.2",
    "vue-tsc": "^0.33.9"
  }
}
