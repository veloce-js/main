{
  "name": "@velocejs/fastapi",
  "version": "0.0.1",
  "description": "Built on top of @velocejs/server to let you build your API in no time",
  "main": "dist/index.js",
  "module": "dist/index.ts",
  "types": "dist/types.d.ts",
  "files": [
    "dist",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "test:ava": "ava",
    "test": "pnpm build && pnpm test:ava",
    "build": "pnpm clean:dist && pnpm build:tsc",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "build:tsc": "tsc -p tsconfig.json",
    "ts:check": "tsc -noEmit",
    "docs": "typedoc --entryPoints src/main.ts",
    "ts": "node -r esbuild-register",
    "ts-node": "ts-node",
    "ts-swc": "node ./swc.js",
    "clean:dist": "rimraf ./dist",
    "prepublishOnly": "pnpm clean:dist && pnpm build:tsc && pnpm test",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "author": "Joel Chu <joelchu@foxmail.com>",
  "license": "ISC",
  "dependencies": {
    "@velocejs/server": "workspace:^0.5.1",
    "debug": "^4.3.4",
    "mime-types": "^2.1.35",
    "parse-multipart-data": "^1.2.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "ava": "^4.1.0",
    "esbuild": "^0.14.30",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.12.0",
    "node-fetch": "^3.2.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "ava": {
    "verbose": true,
    "files": [
      "tests/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "esbuild-register"
    ]
  },
  "homepage": "https://velocejs.com"
}
