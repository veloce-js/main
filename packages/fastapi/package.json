{
  "name": "@velocejs/fastapi",
  "version": "0.9.3",
  "description": "Built on top of @velocejs/server that let you build your API in no time",
  "main": "dist/index.js",
  "module": "dist/index.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "test:ava": "ava",
    "test:nyc": "nyc ava",
    "test": "pnpm build && pnpm test:nyc",
    "build": "pnpm clean:dist && pnpm build:tsc",
    "prepare:types": "cp ./src/types.d.ts ./index.d.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "build:tsc": "tsc -p tsconfig.json",
    "ts:check": "tsc -noEmit",
    "docs": "typedoc --entryPoints src/main.ts",
    "ts": "node -r esbuild-register",
    "ts:swc-node": "echo 'useless' && npx swc-node",
    "test:form": "DEBUG=velocejs:* pnpm ts -- ./examples/form/index.ts",
    "test:upload": "DEBUG=velocejs:* pnpm ts -- ./examples/upload/index.ts",
    "test:vali": "DEBUG=velocejs:* pnpm ts -- ./examples/validation/my-example.ts",
    "test:socket": "pnpm ts ./tests/fixtures/run-websocket.ts",
    "ts-node": "ts-node",
    "clean:dist": "rimraf ./dist",
    "update:version": "node ../../update-devs.js fastapi",
    "prepublishOnly": "pnpm prepare:types && pnpm test",
    "postpublish": "pnpm update:version",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "author": "Joel Chu <joelchu@foxmail.com>",
  "license": "ISC",
  "dependencies": {
    "@jsonql/ast": "^0.8.6",
    "@jsonql/contract": "^0.11.0",
    "@jsonql/errors": "^0.8.0",
    "@jsonql/utils": "^0.8.0",
    "@jsonql/validators": "^0.9.1",
    "@velocejs/bodyparser": "workspace:^0.8.2",
    "@velocejs/config": "workspace:^0.5.2",
    "@velocejs/server": "workspace:^0.9.0",
    "debug": "^4.3.4",
    "fs-extra": "^10.1.0",
    "mime-types": "^2.1.35",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.198",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.42",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "ava": "^4.3.0",
    "esbuild": "^0.14.43",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.17.0",
    "node-fetch": "^3.2.6",
    "nyc": "^15.1.0",
    "open": "^8.4.0",
    "rimraf": "^3.0.2",
    "swc-node": "^1.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "ws": "^8.8.0"
  },
  "ava": {
    "verbose": true,
    "files": [
      "tests/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "esbuild-register"
    ]
  },
  "homepage": "https://velocejs.com"
}
