{
  "name": "@veloce/server",
  "version": "0.0.1",
  "description": "Server core using uWebsocket.js with additional helpers",
  "main": "index.js",
  "files": [
    "dist",
    "src",
    "index.js",
    "index.d.ts",
    "index.ts"
  ],
  "scripts": {
    "test": "ava",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "build:tsc": "tsc -p tsconfig.json",
    "build": "esbuild index.ts --platform=node --bundle --outfile=dist/index.js",
    "clean": "node ./clean.js",
    "docs": "typedoc --entryPoints src/main.ts",
    "ts": "node -r esbuild-register"
  },
  "author": "NEWBRAN LTD & TO1SOURCE",
  "license": "ISC",
  "dependencies": {
    "mime-types": "^2.1.35",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.6.0"
  },
  "devDependencies": {
    "fs-extra": "^10.0.0",
    "tslib": "^1.14.1",
    "@ava/typescript": "^1.1.1",
    "@types/fs-extra": "^9.0.11",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "ava": "^3.15.0",
    "esbuild": "^0.12.0",
    "esbuild-register": "^2.5.0",
    "eslint": "^7.27.0",
    "node-fetch": "^3.2.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "esbuild-register"
    ]
  }
}
