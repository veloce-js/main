    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/server-renderer": "^3.2.37",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dep-import-type": "link:./dep-import-type",
    "express": "^4.18.1",
    "happy-dom": "^5.0.0",
